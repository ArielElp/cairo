//! > module_code
#[inline(always)]
fn bar(data: Array::<felt>) -> Array::<felt> {
     match get_gas() {
          Option::Some(_) => {},
          Option::None(_) => {
              let mut err_data = array_new();
              array_append(ref err_data, 'Out of gas');
              panic(err_data)
          },
      }

      let mut arr = array_new();

      arr
}

